		for($i=0; $i<count($lista_comp['csv']); $i++) {
			$lista_comp['csv'][$i] = str_replace ("\#303\#263", "ó", $lista_comp['csv'][$i]);
		}
		for($i=0; $i<count($lista_comp['kml']); $i++) {
			$lista_comp['kml'][$i] = str_replace ("\#303\#263", "ó", $lista_comp['kml'][$i]);
		}

